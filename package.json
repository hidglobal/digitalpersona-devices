{
  "name": "device-access.js",
  "version": "0.0.1",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "description": "JavaScript Client for DigitalPersona Device Access API",
  "author": "Andrey Bronnikov",
  "license": "(c) HID Global 2019. All rights reserved.",
  "keywords": [
    "DigitalPersona",
    "authentication",
    "identification",
    "password",
    "fingerprints",
    "card",
    "smartcard",
    "proximity card",
    "contactless card",
    "face recognition",
    "face",
    "u2f",
    "fido",
    "pin",
    "push notifications",
    "sms",
    "one-time password",
    "otp",
    "totp",
    "windows integrated authentication",
    "wia",
    "security questions"
  ],
  "module": "./dist/es5/index.js",
  "main": "./dist/es5.bundles/access-management.umd.js",
  "browser": "./dist/es5.bundles/access-management.umd.js",
  "esnext": "./dist/esnext/index.js",
  "typings": "./dist/typings/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run tsc-all && npm run rollup-all",
    "tsc": "tsc",
    "tsc-es5": "tsc --p ./build/tsconfig/tsconfig.es5.json",
    "tsc-esnext": "tsc --p ./build/tsconfig/tsconfig.esnext.json",
    "tsc-types": "tsc --p ./build/tsconfig/tsconfig.types.json",
    "tsc-all": "npm run tsc-es5 && npm run tsc-esnext && npm run tsc-types",
    "rollup": "rollup -c rollup.config.js",
    "rollup-min": "rollup -c rollup.config.js --environment minify:true",
    "rollup-ts": "rollup -c ./build/rollup/rollup.config.ts.js",
    "rollup-ts-min": "rollup -c ./build/rollup/rollup.config.ts.js --environment minify:true",
    "rollup-es5": "rollup -c ./build/rollup/rollup.config.es5.js",
    "rollup-es5-min": "rollup -c ./build/rollup/rollup.config.es5.js --environment minify:true",
    "rollup-esnext": "rollup -c ./build/rollup/rollup.config.esnext.js",
    "rollup-esnext-min": "rollup -c ./build/rollup/rollup.config.esnext.js --environment minify:true",
    "rollup-all": "npm run rollup-es5 && npm run rollup-es5-min && npm run rollup-esnext && npm run rollup-esnext-min",
    "test": "karma start",
    "test-browser": "karma start --single-run",
    "test-node": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine-node.js",
    "test-all": "npm run test-browser && npm run test-node",
    "test-private": "set UUT=private&& karma start",
    "test-common": "set UUT=common&& karma start",
    "test-fp": "set UUT=fp&& karma start",
    "test-card": "set UUT=card&& karma start",
    "test-exclude-uncompressed": "set EXCLUDE_UNCOMPRESSED=true&& karma start",
    "test-node-exclude-uncompressed": "set EXCLUDE_UNCOMPRESSED=true&& ts-node node_modules/jasmine/bin/jasmine --config=jasmine-node.js",
    "docs": "typedoc --options ./build/typedoc.config.js ./src",
    "publish": "npm prune --production && npm publish"
  },
  "bundledDependencies": [],
  "dependencies": {
    "tslib": "^1.9.3",
    "@digitalpersona/access-management": "^0.0.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.9",
    "fetch-mock": "^7.3.1",
    "jasmine": "^3.3.1",
    "jasmine-core": "^3.3.0",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-typescript": "^4.0.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.20.1",
    "terser": "^3.17.0",
    "tslint": "^5.14.0",
    "typedoc": "^0.14.2",
    "typescript": "3.3.3333"
  }
}
